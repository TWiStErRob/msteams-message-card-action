  "homepage": "https://github.com/simbo/msteams-message-card-action#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simbo/msteams-message-card-action.git"
  },
  "bugs": {
    "url": "https://github.com/simbo/msteams-message-card-action/issues"
  },
  "private": true,
  "engines": {
    "node": "^16"
